  ALL_CPPFLAGS += $(CPPFLAGS) -MMD -MP $(DEFINES) $(INCLUDES)
  ALL_CFLAGS += $(CFLAGS) $(ALL_CPPFLAGS) -m32 -g -msse2 -fvisibility=hidden -std=c++11
  ALL_CXXFLAGS += $(CXXFLAGS) $(ALL_CFLAGS)
  ALL_RESFLAGS += $(RESFLAGS) $(DEFINES) $(INCLUDES)
  LIBS += ../Bin/Debug/x86/libFireRaysD.so ../Bin/Debug/x86/libCLWD.a -lOpenCL -lOpenImageIO -lglut -lGLEW -lGL -lpthread
